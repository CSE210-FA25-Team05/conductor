{
  "source": {
    "include": ["backend", "frontend"],
    "exclude": [
        "frontend/svelte.config.js", 
        "frontend/vite.config.js", 
        "node_modules", 
        "frontend/node_modules", 
        "backend/node_modules"
    ],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
