{
  "source": {
    "include": ["backend", "frontend"],
    "exclude": ["node_modules", "backend/node_modules", "frontend/node_modules", "docs"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}