{
  "source": {
    "include": ["backend/src", "frontend/src"],
    "exclude": ["**/node_modules/**", "backend/prisma","**/@prisma/**"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}